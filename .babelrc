{
  "ignore": [
    "**/*.spec.js",
    "**/*.tape.js",
    "**/*.stories.js"
  ],
  "plugins": [
    "transform-export-extensions",
    "transform-class-properties",
    "transform-flow-strip-types",
    "transform-react-jsx",
    "syntax-jsx"
  ],
  "env": {
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }]
      ]
    }
  }
}
