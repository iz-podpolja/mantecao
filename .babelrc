{
  "presets": [
    "es2015",
    "react",
    "flow"
  ],
  "plugins": [
    "polished",
    "transform-export-extensions",
    "transform-class-properties"
  ],
  "env": {
    "production": {
      "presets": [
        ["es2015", {"modules": false}],
        "react",
        "flow"
      ],
      "ignore": [
        "**/*.stories.js",
        "**/*.spec.js",
        "**/*.tape.js"
      ],
      "plugins": [
        "polished",
        "transform-export-extensions",
        "transform-class-properties"
      ]
    }
  }
}
