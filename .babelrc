{
  "presets": [
    [
      "es2015", {
        "modules": false
      }
    ],
    "react",
    "flow"
  ],
  "plugins": [
    "external-helpers",
    "transform-export-extensions",
    "transform-class-properties"
  ],
  "env": {
    "commonjs": {
      "presets": ["es2015", "react", "flow"],
      "plugins": [
        "transform-class-properties"
      ]
    }
  }
}
