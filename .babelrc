{
  "presets": [
    [
      "es2015", {
        "modules": false
      }
    ],
    "react",
    "flow"
  ],
  "ignore":[
    "**/*.stories.js",
    "**/*.spec.js",
    "**/*.tape.js"
  ],
  "env": {
    "commonjs": {
      "presets": ["es2015", "react", "flow"],
      "plugins": [
        "transform-class-properties",
        "transform-export-extensions"
      ]
    },
    "es": {
      "plugins": [
        "transform-export-extensions",
        "transform-class-properties"
      ]
    },
    "rollup": {
      "plugins": [
        "external-helpers",
        "transform-export-extensions",
        "transform-class-properties"
      ]
    }
  }
}
