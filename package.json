{
  "name": "@resmio/mantecao",
  "version": "0.0.0-semantically-released",
  "description": "A react UI library for resmio",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "commit": "git cz",
    "prebuild": "yarn run clean:builds",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=rollup NODE_ENV=development rollup -c -o dist/mantecao.js",
    "build": "yarn run build:icon-index && yarn run build:babel",
    "build:babel": "NODE_ENV=release babel ./src --out-dir ./dist --ignore stories.js,spec.js,tape.js",
    "build:icon-index": "babel-node ./scripts/icon-index-generator.js",
    "clean:builds": "rimraf dist es lib",
    "precommit": "yarn run prettier && yarn run lint",
    "prepush": "yarn test",
    "lint": "eslint ./src",
    "start": "yarn run storybook:dev",
    "storybook:dev": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "rimraf docs && build-storybook -c .storybook -o docs",
    "test": "jest",
    "prettier": "prettier --single-quote --no-semi \"src/**/*.js\" --write",
    "prettier:test": "prettier --single-quote --no-semi \"src/**/*.js\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/resmio/mantecao.git"
  },
  "keywords": [
    "react",
    "ui",
    "library",
    "components",
    "dialog",
    "resmio"
  ],
  "author": "resmio <developers@resmio.com> (https://resmio.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/resmio/mantecao/issues"
  },
  "homepage": "https://resmio.github.io/mantecao",
  "release": {
    "verifyConditions": "condition-circle"
  },
  "peerDependencies": {
    "@resmio/rollico": "^3.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-event-listener": "0.x"
  },
  "devDependencies": {
    "@storybook/react": "^3.1.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.9.6",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.0.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.48.0",
    "glamor": "^2.20.25",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "prettier": "^1.4.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "semantic-release": "^6.3.6"
  },
  "directories": {
    "doc": "docs"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
